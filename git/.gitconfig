[core]
	editor = nvim
[color]
	diff = auto
	status = auto
	branch = auto
	pager = true
[pull]
	rebase = true
[filter "lfs"]
	clean = git-lfs clean -- %f
	smudge = git-lfs smudge -- %f
	process = git-lfs filter-process
	required = true
